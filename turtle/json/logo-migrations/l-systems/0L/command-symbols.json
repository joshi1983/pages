{
	"+": {
		"description": "Turns left by angle",
		"to": "left :angle"
	},
	"-": {
		"description": "Turns right by angle",
		"to": "right :angle"
	},
	"F": {
		"description": "Move forward by line length drawing a line",
		"to": "forward :length"
	},
	"f": {
		"to": "jumpForward :length"
	},
	"|": {
		"to": "right 180"
	},
	"[": {
		"to": "pushState"
	},
	"]": {
		"to": "popState"
	},
	"#": {
		"to": "incrementLineWidth"
	},
	"!": {
		"to": "decrementLineWidth"
	},
	"@": {
		"to": "circle :lineWidth / 2"
	},
	"{": {
		"to": "polyStart"
	},
	"}": {
		"to": "polyEnd"
	},
	">": {
		"to": "make \"length :length * :lengthScaleFactor"
	},
	"<": {
		"to": "make \"length :length / :lengthScaleFactor"
	},
	"&": {
		"to": "make \"angle -:angle"
	},
	"(": {
		"to": "decrementTurningAngle"
	},
	")": {
		"to": "incrementTurningAngle"
	}
}
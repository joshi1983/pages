{
	"name": "Mini-Turtle Logo",
	"description": "A Logo interpreter and programming environment in a desktop application",
	"externalLink": "https://www.youtube.com/watch?v=qTjDy0z6U3s",
	"commands": [
		{"primaryName": "clr",
		"to": "clearScreen"},
		{"primaryName": "fwd",
		"to": "forward"},
		{"primaryName": "getX",
		"to": "xCor"},
		{"primaryName": "getY",
		"to": "yCor"},
		{"primaryName": "home",
		"to": "home"},
		{"primaryName": "if",
		"to": "if"},
		{"primaryName": "on",
		"to": "to"},
		{"primaryName": "penDn",
		"to": "penDown"},
		{
			"primaryName": "penRGB",
			"args": [
				{"name": "red", "types": "num"},
				{"name": "green", "types": "num"},
				{"name": "blue", "types": "num"}
			],
			"to": "setPenColor",
			"translateAllParametersToSingleColor": true,
			"description": "Sets pen color.  All arguments should be combined into 1 list."
		},
		{"primaryName": "penSz",
		"to": "setPenSize"},
		{"primaryName": "penUp",
		"to": "penUp"},
		{"primaryName": "repeat",
		"to": "repeat"},
		{"primaryName": "rot",
		"to": "left"},
		{"primaryName": "set",
		"to": "make"}
	],
	"operators": [
		{"symbol": "-", "to": "-"},
		{"symbol": "<=", "to": "<="}
	],
	"keywords": [
		{"from": "then", "toSymbol": "["}
	]
}
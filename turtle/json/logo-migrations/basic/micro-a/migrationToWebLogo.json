{
	"name": "Micro(A) BASIC",
	"description": "A dialect of BASIC that runs in Microsoft Windows.  It is for expressing programs that draw pictures and react to Windows events, mouse input, keyboard input...",
	"externalLink": "https://sourceforge.net/projects/micro-a-interpreter/",
	"functions": [
		{
			"name": "bcolor",
			"args": [
				{"name": "red", "types": "int"},
				{"name": "green", "types": "int"},
				{"name": "blue", "types": "int"}
			],
			"description": "Looks very similar to what wcolor does according to ReadMe.txt from a downloaded Micro(A) BASIC zip file",
			"translateAllParametersToSingleColor": true,
			"to": "setScreenColor"
		},
		{
			"name": "chrs",
			"args": [
				{"name": "num"}
			],
			"to": "char"
		},
		{
			"name": "fcolor",
			"args": [
				{"name": "red", "types": "int"},
				{"name": "green", "types": "int"},
				{"name": "blue", "types": "int"}
			],
			"translateAllParametersToSingleColor": true,
			"to": "setFillColor"
		},
		{
			"name": "rand",
			"to": "random"
		},
		{
			"name": "rnd",
			"args": [],
			"to": "randomRatio"
		},
		{
			"name": "sqr",
			"to": "sqrt"
		},
		{
			"name": "wcolor",
			"args": [
				{"name": "red", "types": "int"},
				{"name": "green", "types": "int"},
				{"name": "blue", "types": "int"}
			],
			"translateAllParametersToSingleColor": true,
			"to": "setScreenColor"
		}
	]
}
to emojiBackground :radius
	localmake "oldState turtleState
	localmake "oldPos pos
	localmake "arcRadius1 :radius * 1.5
	localmake "arcRadius2 :radius * 0.28
	localmake "arcRadius3 :radius * 0.9094244
	localmake "angle1 25
	localmake "angle2 100
	localmake "angle3 110
	localmake "coreColor "#FDF03F
	localmake "outerColor1 "orange
	localmake "outerColor2 "#FF7B00
	localmake "colorStops createPList
	setProperty "colorStops 0 :coreColor
	setProperty "colorStops 0.75 mix :coreColor :outerColor1 0.8
	setProperty "colorStops 0.95 :outerColor1
	setProperty "colorStops 1 :outerColor2
	setFillGradient createRadialGradient pos pos :radius :colorStops "pad
	setPenSize :radius * 0.002
	setPenColor "brown
	circle :radius

	; darken the upper part of the circle.
	localmake "colorStops createPList
	setProperty "colorStops 0 mix :outerColor1 transparent 0.3
	setProperty "colorStops 1 transparent
	jumpForward :radius
	localmake "fromPos pos
	jumpBackward :radius * 1.4
	setFillGradient createLinearGradient :fromPos pos :colorStops "pad
	setPenSize 0
	jumpTo :oldPos
	circle :radius

	; Draw some reflected light from above.
	localmake "colorStops createPList
	setProperty "colorStops 0 "#0fff
	setProperty "colorStops 1 "#dfff
	jumpForward :radius
	localmake "fromPos pos
	jumpBackward :radius * 1.2
	setFillGradient createLinearGradient  pos :fromPos :colorStops "pad
	jumpTo :oldPos
	right 90
	polyStart
	arcLeft :angle1 :arcRadius1
	arcLeft :angle2 :arcRadius2
	arcLeft :angle3 :arcRadius3
	arcLeft :angle2 :arcRadius2
	arcLeft :angle1 :arcRadius1
	polyEnd
	setTurtleState :oldState
end

to fillHeart :height
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "size1 :height * 0.7

	; Some trigonometry used to calculate exact sizes for symmetry.
	localmake "arcRadius (:height - :size1 * cos 45) / (1 + cos 45)
	localmake "size2 (:size1 - :arcRadius * 2) 
	right 45
	polyStart
	jumpForward :size1
	arcLeft 180 :arcRadius
	jumpForward :size2
	right 90
	jumpForward :size2
	arcLeft 180 :arcRadius
	polyEnd
	jumpTo :oldPos
	setHeading :oldHeading
end

to redHeart :height

	localmake "height2 :height * 0.95
	setFillColor "#dfff
	jumpTo :oldPos
	jumpBackward :height * 0.5
	fillHeart :height2

	setFillColor "#8840
	jumpTo :oldPos
	jumpBackward :height * 0.43
	fillHeart :height1

	setFillColor "black
	jumpTo :oldPos
	jumpBackward :height * 0.45
	fillHeart :height1

	localmake "colorStops createPList
	setProperty "colorStops 0 "red
	setProperty "colorStops 0.4 "#E00000
	setProperty "colorStops 0.8 "darkRed
	setProperty "colorStops 1 "#750000
	setFillGradient createRadialGradient :oldPos :oldPos :height2 * 0.7 :colorStops "pad
	jumpTo :oldPos
	jumpBackward :height2 * 0.5
	fillHeart :height2
	jumpTo :oldPos
end


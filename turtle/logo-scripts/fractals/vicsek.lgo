; The Vicsek fractal is explained at:
; https://en.wikipedia.org/wiki/Vicsek_fractal
to vicsek :size
	ifelse :size < 3 [
		square :size * 3
	] [
		localmake "oldPos pos
		localmake "oldHeading heading
		vicsek :size / 3
		repeat 2 [
			jumpForward :size
			vicsek :size / 3
			jumpBackward :size * 2
			vicsek :size / 3
			jumpTo :oldPos
			right 90
		]
		setHeading :oldHeading
	]
end

setPenSize 0
setFillColor "black
vicsek 150
to drawInnerShape :height
	localmake "oldState turtleState
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "lines1 [
		[30 0] [0.3] [-20 0] [0.51] [-55.074564 0]
		[0.3369374] [-44.92544 0]
	]
	setPenSize 0
	setFillColor "#abdaf4
	drawArcLineShape [0 0
		(combine :lines1 reverse :lines1)
	] :height

	jumpTo :oldPos
	setHeading :oldHeading
	jumpForward :height * 0.5
	setFillColor "#275c7b
	ellipse :height * 0.013 :height * 0.4
	setTurtleState :oldState
end


to niveeCyberSecurityLogo :height
	localmake "oldState turtleState
	localmake "oldHeading heading
	localmake "numDashes 67
	localmake "redRadius :height * 0.405
	localmake "innerShapeHeight :height * 0.22
	localmake "numInnerShapes 16
	jumpForward :height / 2
	localmake "center pos
	setPenSize 0
	setFillColor "#ce3635
	circle :redRadius
	setPenSize :height * 0.025
	setLineCap "round
	setPenGradient createRadialGradient pos :height / 2 createPList2 [
		[0.83 "white] [1 "#426777]
	]
	repeat :numDashes [
		setHeading :oldHeading + 4.5 + repcount * 360 / :numDashes
		jumpForward (:height - penSize) / 2
		left 150
		forward :height * 0.06
		jumpTo :center
	]
	repeat :numInnerShapes [
		setHeading :oldHeading + repcount * 360 / :numInnerShapes
		jumpForward :redRadius - :innerShapeHeight
		drawInnerShape :innerShapeHeight
		jumpTo :center
	]
	setTurtleState :oldState
end


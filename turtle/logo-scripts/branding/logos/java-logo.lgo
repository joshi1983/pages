to javaCupSteam :height
	localmake "oldState turtleState
	localmake "oldPos pos
	localmake "oldHeading heading
	setFillColor "#F8981D
	jumpForward :height * 0.4501
	jumpRight :height * 0.0051
	left 48.8
	setPenSize 0

	; Left side swirl shape
	polyStart
	arcRight 5 :height
	arcRight 8 :height * 0.45
	arcRight 15 :height * 0.18

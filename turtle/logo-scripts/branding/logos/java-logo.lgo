	arcRight 15 :height * 0.092
	arcRight 25 :height * 0.14
	arcRight 15 :height * 0.16
	arcRight 15 :height * 0.355
	arcRight 6.5 :height * 0.6
	arcLeft 20 :height * 0.25
	arcLeft 50 :height * 0.1007
	arcLeft 15 :height * 0.16
	arcLeft 2 :height
	jumpForward :height * 0.03
	arcRight 9 :height * 0.27
	polyEnd

	; Right side swirl shape
	jumpTo :oldPos
	setHeading :oldHeading
	jumpForward :height * 0.4152294
	jumpRight :height * 0.0373569
	right 48.5
	polyStart
	arcLeft 24 :height * 0.103

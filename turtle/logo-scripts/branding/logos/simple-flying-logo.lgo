; Inspired by logo at
; https://simpleflying.com
to simpleFlyingLogo :height
	localmake "oldState turtleState
	localmake "width :height * 0.52
	setPenColor "#2a7de2
	setPenSize :height * 0.098
	setFillColor transparent
	jumpForward :height / 2
	roundRect :width :height - penSize :width / 2
	setTurtleState :oldState
end


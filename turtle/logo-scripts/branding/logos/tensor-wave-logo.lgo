; Inspired by logo at:
; https://www.youtube.com/@TensorWaveCloud
to tensorWaveLogo :height
	localmake "oldState turtleState
	localmake "len1 0.1
	localmake "arcAngle 70
	localmake "lines1 [
		[-90 0] [:len1] [-:arcAngle 0.19] [0.13]
		[:arcAngle 0.2] [:len1]
		[90 0] [0.23]
	]
	stripes :height :height ["black]
	setPenSize 0
	setFillColor "#0de2f2
	drawArcLineShape [-0.303 0.575
		(combine :lines1 [[180 0]] :lines1)
	] :height

	setTurtleState :oldState
end


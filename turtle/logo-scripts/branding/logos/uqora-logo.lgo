to uqoraLogo :height
	localmake "oldState turtleState
	localmake "lines1 [
		[90 0] [0.99] [-91 0.017] [-45 0.995] [-44 1.012489105]
	]
	setPenSize 0
	setFillColor "#FE633C
	drawArcLineShape [0 0
		combine :lines1 reverse :lines1
	] :height
	setTurtleState :oldState
end


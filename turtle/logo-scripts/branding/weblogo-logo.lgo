to triangle :size
	localmake "size1 :size * sqrt 2
	setpensize :size * 0.2
	right 90
	forward :size
	left 135
	forward :size1
	left 90
	forward :size1
	left 135
	forward :size
	left 90
	closePath
end

to turtle :size
	setpencolor "black
	triangle :size
	setpencolor "green
	setpensize 0
	fd :size * 0.1
	triangle :size * 0.8
end

right 90
turtle 100
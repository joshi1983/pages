; Inspired by graphics on 
; The Coding Train website
; https://thecodingtrain.com/

; His videos mostly involve JavaScript but
; he has an interesting video on creating 
; a very basic Logo interpreter.
; https://www.youtube.com/watch?v=flHxQpyiDNc

; The animation rotates the train's smoke clouds, wheels,
; and sways the conductor image a bit to give the impression
; of waving and small movement.

make "cartColor "lightSkyBlue

; Raw PNG data for the conductor's image
make "codingTrainConductor 'data:image/png;base64,'

make "rainbowColors [
	"#CE4BCE "#FF94E2 :cartColor
	"yellow "orange "red
]

to spiral :size
	localmake "oldPos pos
	localmake "oldHeading heading
	jumpForward :size
	right 90
	repeat 8 [
		arcRight 90 :size
		localmake "size :size * 0.8
	]
	jumpTo :oldPos
	setHeading :oldHeading
end

to backgroundGradient :width :height
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "oldPenColor penColor
	localmake "oldPenSize penSize

	localmake "colorStops createPList2 [
		[0 "#A2C3A2] [0.2 "#F7EBFA] [0.25 "#F5FFFF]
		[0.26 "#FFFFFF] [1 "#F0F9FF]
	]
	jumpForward :height
	setPenGradient createLinearGradient :oldPos pos :colorStops "pad
	setPenSize :width
	setLineCap "butt
	backward :height
	setPenColor :oldPenColor
	jumpTo :oldPos
	setHeading :oldHeading
	setPenSize :oldPenSize
end

to rainbow :size :travelDistance
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "oldPenColor penColor
	localmake "oldPenSize penSize
	localmake "arcAngle 140
	localmake "arcRadius :size * 0.587
	localmake "numColors count :rainbowColors
	setPenSize :size * 0.075
	jumpForward penSize * 0.5
	jumpLeft penSize * (:numColors - 1) * 0.5
	repeat :numColors [
		localmake "pos1 pos
		setPenColor item repcount :rainbowColors
		arcLeft :arcAngle :arcRadius
		localmake "endPos pos
		jumpTo :pos1
		localmake "arcRadius :arcRadius + penSize
		setHeading :oldHeading
		jumpRight penSize
	]
	jumpTo :endPos
	setPenColor :oldPenColor
	setPenSize :oldPenSize
	jumpRight :size * 0.05
	left 105
	rainbowClouds :size * 0.6 :travelDistance

	; restore original turtle state.	
	jumpTo :oldPos
	setHeading :oldHeading
end

to rainbowSpirals :size :travelDistance
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "oldPenColor penColor
	; position and color of various spirals
	localmake "spiralInfo [
		[0.5 0.8 2]
		[-0.5 0.8 2]
		[0.8 0.6 4]
		[-0.35 0.35 2]
		[0.3 0.3 3]
	]
	localmake "numSpirals count :spiralInfo
	localmake "xScale :size
	localmake "yScale :size * 3.5
	localmake "spiralSize :size * 0.15
	localmake "rotation :travelDistance
	repeat :numSpirals [
		localmake "spiral item repcount :spiralInfo
		localmake "colorIndex item 3 :spiral
		localmake "color item :colorIndex :rainbowColors
		jumpRight :xScale * item 1 :spiral
		jumpForward :yScale * item 2 :spiral
		right :rotation
		setPenColor :color
		spiral :spiralSize
		jumpTo :oldPos
		setHeading :oldHeading
	]
	setPenColor :oldPenColor
end

to rainbowClouds :size :travelDistance
	localmake "oldPos pos
	localmake "oldHeading heading
	rainbowSpirals :size :travelDistance
	repeat 3 [
		localmake "size1 :size * (4 - repcount) / 3
		setHeading :oldHeading + (repcount + 0.02 * :travelDistance) * 45
		cloud2 :size1
		setHeading :oldHeading
		jumpForward :size1 * 2
	]
	setHeading :oldHeading
	jumpTo :oldPos
end

to streak :len
	localmake "oldState turtleState

	setLineCap "round ; round ends of the line
	forward :len
	setPenSize penSize * 0.75
	setPenColor fillColor
	backward :len

	setTurtleState :oldState
end

to eye :size
	localmake "smallRadius :size * 0.5
	ellipseArc 180 :smallRadius :size 90
end

to eyes :size
	localmake "oldState turtleState
	localmake "eyeSize :size * 0.5
	localmake "eyeSeparation :size * 0.95
	setPenSize :size * 0.05
	jumpLeft :eyeSeparation * 0.5
	eye :eyeSize
	jumpRight :eyeSeparation
	eye :eyeSize

	setTurtleState :oldState
end

to smile :size
	setPenSize 0
	drawArcLineShape [1 0
		[[180 0] [-180 1]]] :size
end

to happySun :size
	localmake "oldPos pos
	localmake "oldState turtleState
	localmake "circleRadius :size * 0.6
	localmake "streakInnerRadius :circleRadius * 1.15
	localmake "maxStreakLength :size - :streakInnerRadius
	localmake "orangeColor "#E69500
	setFillColor "yellow
	setPenColor :orangeColor
	setPenSize :size * 0.015
	circle :circleRadius
	left 10 ; rotate the sun as if head is tilting back with enthusiastic laugh
	setPenSize :size * 0.11
	repeat 8 [
		localmake "streakLength :maxStreakLength * (0.5 + 0.5 * remainder repcount 2)
		jumpForward :streakInnerRadius
		streak :streakLength
		jumpTo :oldPos
		right 360 / 8
	]
	; move slightly up and right of the circle's center.
	jumpForward :circleRadius * 0.15
	jumpRight :circleRadius * 0.12
	eyes :circleRadius * 0.9

	jumpTo :oldPos
	jumpRight :circleRadius * 0.1
	setFillColor :orangeColor
	smile :circleRadius * 0.5

	setTurtleState :oldState
end

to trainTrack :len :thickness
	localmake "oldState turtleState
	localmake "numPlanks int (:len * 0.2 / :thickness)
	localmake "plankWidth :len * 0.5 / :numPlanks
	localmake "halfThickness :thickness * 0.5

	setFillColor "#636363
	setPenSize 0
	polyStart
	jumpForward :len
	right 90
	jumpForward :thickness
	repeat :numPlanks [
		right 90
		jumpForward :plankWidth
		jumpRight :halfThickness
		jumpForward :plankWidth
		left 90
		jumpForward :halfThickness
	]
	polyEnd
	setTurtleState :oldState
end

to cloud :size
	localmake "oldState turtleState

	jumpRight :size
	polyStart
	arcsLeft [[40 0.5] [90 0.3] [-120 0] [50 0.6]
	[65 0.35] [55 0.6] [-150 0] [110 0.2619] [40 0.4719]] :size
	polyEnd

	setTurtleState :oldState
end

to cloud2 :size
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "oldPenSize penSize
	localmake "oldState turtleState
	localmake "size1 :size * 0.45
	localmake "size2 :size * 0.5
	setFillColor "white
	setPenSize 0
	; fill the cloud background
	repeat 3 [
		jumpForward :size1
		circle :size2
		jumpTo :oldPos
		right 120
	]

	; draw the outline of the clouds.
	repeat 3 [
		jumpForward :size1 - :size2
		left 90
		setPenSize 0
		arcRight 70 :size2
		setPenSize :oldPenSize
		arcRight 250 :size2
		jumpTo :oldPos
		setHeading :oldHeading + repcount * 120
	]

	setTurtleState :oldState
end

to wheel :radius :rimThickness
	localmake "oldPos pos
	localmake "oldPenSize penSize
	localmake "radius1 :radius - penSize * 0.5
	localmake "blueColor :cartColor
	localmake "centerRadius :radius * 0.25
	localmake "innerRimRadius :radius1 - :rimThickness
	setPenSize :rimThickness
	setPenColor :blueColor
	setFillColor "white
	circle :radius - :rimThickness * 0.5

	setPenColor "black
	setPenSize :oldPenSize
	setFillColor :blueColor
	circle :centerRadius
	setFillColor transparent
	circle :radius1
	circle :innerRimRadius

	; draw spokes.
	repeat 8 [
		jumpForward :centerRadius
		forward :innerRimRadius - :centerRadius
		jumpTo :oldPos
		right 360 / 8
	]

	setPenSize :oldPenSize
end

to rotatedWheel :radius :rimThickness :circumferenceRotation
	localmake "rotationAngle :circumferenceRotation * 180 / pi / :radius
	right :rotationAngle
	wheel :radius :rimThickness
	left :rotationAngle
end

to myRect :width :height
	localmake "oldPos pos
	jumpForward :height / 2	
	rect :width :height
	jumpTo :oldPos
end

to cart :size :travelDistance
	localmake "oldPos pos
	localmake "width1 :size * 0.95
	localmake "wheelRadius :size * 0.14
	localmake "wheelSeparation :wheelRadius * 3.5
	localmake "rimThickness :wheelRadius * 0.2
	localmake "cartHeight :size * 0.35

	jumpForward :wheelRadius
	setFillColor :cartColor
	jumpRight :width1
	myRect :width1 :size * 0.05
	jumpLeft :width1
	myRect :width1 :cartHeight
	jumpForward :cartHeight
	myRect :size :size * 0.04
	jumpBackward :cartHeight

	; draw wheels
	jumpLeft :wheelSeparation * 0.5
	rotatedWheel :wheelRadius :rimThickness :travelDistance
	jumpRight :wheelSeparation
	rotatedWheel :wheelRadius :rimThickness :travelDistance

	jumpTo :oldPos
end

to engineVentSlanted :size
	localmake "oldHeading heading
	localmake "oldState turtleState
	localmake "angle 40
	localmake "height :size
	localmake "ventHeight :height * 0.55
	localmake "ventBaseHeight :height * 0.22
	localmake "ventLength :ventHeight / cos :angle
	localmake "gap :size * 0.23
	parallelogram :size :height / cos :angle -:angle
	setLineCap "round
	setPenSize :size * 0.05
	left :angle
	jumpForward :ventBaseHeight
	setHeading :oldHeading
	jumpLeft :gap
	repeat 3 [
		setHeading :oldHeading - :angle
		forward :ventLength
		jumpBackward :ventLength
		setHeading :oldHeading
		jumpRight :gap
	]
	setTurtleState :oldState
end

to tinyEngineWheels :width :height :travelDistance
	localmake "oldState turtleState
	localmake "tinyWheelRadius :height * 0.45
	localmake "size1 :width * 0.5 - :tinyWheelRadius
	setFillColor :cartColor
	jumpForward :tinyWheelRadius
	myRect :width * 0.95 :height - :tinyWheelRadius
	jumpRight :size1
	rotatedWheel :tinyWheelRadius 0 :travelDistance
	jumpLeft :size1 * 2
	rotatedWheel :tinyWheelRadius 0 :travelDistance

	setFillColor :cartColor
	setPenSize :tinyWheelRadius * 0.8
	right 90
	streak :size1 * 2

	setTurtleState :oldState
end

to ventedEngine :width  :animationCycle
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "oldPenSize penSize
	localmake "oldState turtleState
	localmake "angle 10
	localmake "height :width * 0.6
	localmake "slantedLength :height * 0.5 / cos :angle
	localmake "ventLineHeight :height * 0.5
	localmake "gap :width * 0.12
	setFillColor :cartColor
	polyStart
	jumpRight :width
	right :angle
	jumpForward :slantedLength
	left :angle * 2
	jumpForward :slantedLength
	left 90 - :angle
	jumpForward :width
	left 90
	jumpForward :height
	polyEnd

	setLineCap "round
	setPenSize :width * 0.025
	jumpTo :oldPos
	setHeading :oldHeading
	jumpRight :width * 0.92
	jumpForward (:height - :ventLineHeight) * 0.5
	repeat 4 [
		forward :ventLineHeight
		jumpBackward :ventLineHeight
		jumpLeft :gap
	]
	jumpTo :oldPos
	setHeading :oldHeading
	jumpForward :height
	jumpRight :width * 0.75
	setPenSize :oldPenSize
	chimney :width :animationCycle
	jumpLeft :width * 0.5
	tinyEngineCap :height * 0.4

	setTurtleState :oldState
end

to chimney :size :animationCycle
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "oldPenSize penSize
	localmake "oldState turtleState
	localmake "height :size
	localmake "height1 :height * 0.92
	localmake "rimHeight :height - :height1
	localmake "baseWidth :size * 0.4
	localmake "topWidth :baseWidth + 2 * :height1 * 0.0875

	setLineJoinStyle "miter
	jumpForward :height1
	rainbow :size * 1.3 :animationCycle
	setPenSize :oldPenSize
	jumpTo :oldPos
	jumpForward :height1
	myRect :topWidth * 1.1 :rimHeight

	setHeading :oldHeading
	jumpTo :oldPos
	setFillColor :cartColor

	isoTrapezoid :baseWidth :topWidth :height1

	setTurtleState :oldState
end

to tinyEngineCap :height
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "height1 :height * 0.8
	localmake "arcRadius :height - :height1
	localmake "width :height * 0.7
	jumpRight :width * 0.5
	polyStart
	jumpForward :height1
	arcLeft 90 :arcRadius
	jumpForward :width - :arcRadius * 2
	arcLeft 90 :arcRadius
	jumpForward :height1
	polyEnd

	setHeading :oldHeading
	jumpTo :oldPos
end

to drawConductor :size :travelDistance
	; Animate the angle to give the impression of 
	; the conductor waving or moving subtly.
	localmake "angle 5 + 3 * sin :travelDistance * 9
	left :angle
	image :size :size * 68 / 88 :codingTrainConductor
	right :angle
end

to drawEngineCab :cabWidth :height :travelDistance
	localmake "oldHeading heading
	localmake "oldPos pos
	localmake "oldPenSize penSize
	localmake "windowSize :cabWidth * 0.9
	localmake "oldState turtleState
	localmake "oldPenColor penColor
	localmake "windowTrimSize (:cabWidth - :windowSize) * 0.5
	localmake "windowBottom :height - :windowSize - :windowTrimSize

	setLineJoinStyle "miter
	setLineCap "butt
	setPenSize :cabWidth
	setPenColor :cartColor
	forward :windowBottom
	jumpRight (:windowSize + :windowTrimSize) * 0.5
	setPenSize :windowTrimSize
	forward :windowSize + :windowTrimSize * 0.5
	repeat 2 [
		left 90
		forward :windowSize + :windowTrimSize
	]
	setHeading :oldHeading
	setFillColor transparent
	jumpTo :oldPos

	setPenSize :oldPenSize
	setPenColor :oldPenColor
	myRect :cabWidth :height
	jumpForward :windowBottom
	myRect :windowSize :windowSize
	jumpLeft :cabWidth * 0.3
	jumpBackward :height * 0.05
	drawConductor :cabWidth * 1.1 :travelDistance

	setTurtleState :oldState
end

to engine :size :travelDistance
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "largeWheelRadius :size * 0.2
	localmake "largeWheelRimThickness :largeWheelRadius * 0.2
	localmake "smallHeight :size * 0.11
	localmake "groundClearance1 :size * 0.02
	localmake "groundClearance2 :largeWheelRadius - :smallHeight * 0.5
	localmake "height :size * 0.7
	localmake "cabWidth :size * 0.45
	setFillColor :cartColor
	setPenColor "black

	jumpForward :largeWheelRadius + :smallHeight * 0.5
	jumpLeft :size * 0.094
	ventedEngine :size * 0.42 :travelDistance

	jumpTo :oldPos
	jumpRight :size * 0.4
	jumpForward :groundClearance1
	engineVentSlanted :groundClearance2 + :smallHeight - :groundClearance1

	jumpTo :oldPos
	jumpForward :groundClearance2
	jumpLeft :size * 0.18
	myRect :size * 0.73 :smallHeight
	jumpForward :smallHeight
	jumpLeft :size * 0.14
	drawEngineCab :cabWidth :height :travelDistance

	setHeading :oldHeading
	jumpTo :oldPos
	jumpForward :largeWheelRadius
	jumpLeft :size * 0.35
	rotatedWheel :largeWheelRadius :largeWheelRimThickness :travelDistance
	jumpTo :oldPos
	jumpRight :size * 0.04
	tinyEngineWheels :size * 0.3 :largeWheelRadius - :smallHeight * 0.5 :travelDistance
	jumpTo :oldPos
end

to drawClouds :height
	localmake "oldPos pos
	; cloud position and sizes
	; formatted as:
	; [xPosFactor yPosFactor sizeFactor]
	localmake "cloudInfo [
		[-3 0.6 0.06]
		[-2.7 0.8 0.07]
		[-2.25 0.5 0.12]
		[-1.9 0.9 0.05]
		[-1.5 0.6 0.1]
		[-1.1 0.8 0.08]
		[-1 0.6 0.06]
		[-0.7 0.8 0.07]
		[-0.25 0.5 0.12]
		[0 0.8 0.08]
		[0.2 0.75 0.1]
		[0.9 0.9 0.05]
		[1.5 0.6 0.06]
		[1.8 0.8 0.08]
		[2.2 0.6 0.1]
		[2.6 0.5 0.12]
		[3 0.8 0.08]
		[3.4 0.6 0.1]
	]
	setFillColor "white
	setPenSize :height * 0.002
	repeat count :cloudInfo [
		localmake "p item repcount :cloudInfo
		jumpRight :height * item 1 :p
		jumpForward :height * item 2 :p
		cloud :height * item 3 :p
		jumpTo :oldPos
	]
end

to completeDrawing :size :animationCycle
	localmake "oldPos pos
	localmake "oldHeading heading
	localmake "trackThickness :size * 0.02
	localmake "trackElevation :size * 0.03
	localmake "sunSize :size * 0.1
	localmake "width :size * 8
	localmake "travelDistance :animationCycle * 3 * :size
	backgroundGradient :width :size
	drawClouds :size
	jumpForward :size * 0.45
	jumpLeft :size
	happySun :sunSize
	jumpTo :oldPos
	jumpLeft :width * 0.5
	jumpForward :trackElevation
	right 90
	trainTrack :width :trackThickness
	jumpTo :oldPos
	setHeading :oldHeading
	jumpForward :trackElevation
	jumpRight -2 * :size + :travelDistance
	setPenSize :size * 0.002
	cart :size * 0.5 :travelDistance
	jumpRight :size * 0.65
	engine :size * 0.5 :travelDistance
end

completeDrawing 100 animation.timeRatio

to animation.snapshotstyle
	output createPList2 [
		["position.y 50]
		["zoom.scale 3]
	]
end

to animation.setup
	localmake "minutes 0
	localmake "seconds 10
	; Edit this if you want to change the length of your animation.
	output createPList2 [
		["duration :minutes * 60 + :seconds]
	]
end
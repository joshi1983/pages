to SQUIRAL
	repeat 300 [
		forward repcount * 2
		right 89 + animation.time * 0.5
	]
end

SQUIRAL

to animation.setup
	localmake "minutes 0
	localmake "seconds 20
	; Edit this if you want to change the length of your animation.
	localmake "result createPList
	setProperty "result "duration :minutes * 60 + :seconds
	output :result
end
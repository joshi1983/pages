; pattern created by drawing straight lines between
; every vertex on 16-sides regular polygon

make "numSides 16
make "radius 200
setPenSize 0.5

repeat :numSides [
	make "angle1 repcount * 360 / :numSides
	penUp
	forward :radius
	make "tempPos pos
	penDown
	repeat :numSides [
		make "angle repcount * 360 / :numSides
		setXY :radius * cos :angle :radius * sin :angle
		setPos :tempPos
	]
	home
	right 360 / :numSides
]
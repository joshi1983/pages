; Made to look like:
; https://upload.wikimedia.org/wikipedia/commons/thumb/2/20/Flag_of_the_Netherlands.svg/1200px-Flag_of_the_Netherlands.svg.png

make "width 400
make "height :width * 2 / 3
make "colors ["#ae1c28 "#fff "#21468b]
make "thickness :height / count :colors
setScreenColor "grey
setPenSize :width

repeat count :colors [
	make "c item repcount :colors
	setPenColor :c
	backward :thickness
]
; https://commons.wikimedia.org/wiki/File:Bahai_star.svg
to bahaiStar :height
	localmake "oldState turtleState
	setLineJoinStyle "miter
	setPenSize :height * 0.046
	setPenColor "black
	setFillColor "white
	jumpForward :height * 0.484454
	isotoxalStar :height * 0.4496449 :height * 0.242 9
	setTurtleState :oldState
end

